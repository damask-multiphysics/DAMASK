integer(pInt) num_concom
parameter(num_concom=231)
common/marc_concom/&
 iacous,      iasmbl,    iautth,     ibear,   icompl,     iconj,       icreep,    ideva(60),  idyn,    idynt,&
 ielas,       ielcma,    ielect,     iform,   ifour,      iharm,       ihcps,     iheat,      iheatt,  ihresp,&
